meta {
  name: postRol
  type: http
  seq: 2
}

post {
  url: {{base_url}}/prueba
  body: json
  auth: none
}

body:json {
  {
    "nom_rol": "otro"
  }
}

script:pre-request {
  const { faker } = require('@faker-js/faker');
  
  const randomName = faker.name.fullName();
  req.setBody({
    nom_rol: randomName,
  });
  
}
